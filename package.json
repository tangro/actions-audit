{
  "name": "@tangro/actions-audit",
  "version": "1.1.12",
  "private": true,
  "description": "Run npm audit",
  "main": "lib/main.js",
  "scripts": {
    "build": "ncc build src/main.ts --minify",
    "update-readme": "node scripts/update-readme.js",
    "update-dependencies": "npx npm-check-updates -u",
    "release": "npm version patch -m \"version bump\" && npm run update-readme && git -a -m \"updated README\" && git push",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tangro/tangro-actions-template.git"
  },
  "keywords": [
    "actions"
  ],
  "author": "ankri",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/exec": "1.0.4",
    "@tangro/tangro-github-toolkit": "1.0.3"
  },
  "devDependencies": {
    "@types/node": "14.11.5",
    "@zeit/ncc": "0.22.3",
    "ejs": "3.1.5",
    "typescript": "4.0.3"
  }
}
