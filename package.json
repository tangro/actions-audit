{
  "name": "@tangro/actions-audit",
  "version": "1.1.22",
  "private": true,
  "description": "Run npm audit",
  "main": "lib/main.js",
  "scripts": {
    "build": "rimraf dist && ncc build src/main.ts --minify",
    "update-readme": "node scripts/update-readme.js",
    "update-dependencies": "npx npm-check-updates -u",
    "release": "npm version patch ---git-tag-version && npm run update-readme && git -a -m \"updated README\" && git push",
    "test": "ts-node test/test.ts",
    "version": "npm run build && git add -A",
    "postversion": "npm run update-readme && git add -A && git commit -m \"updated readme\" && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tangro/actions-audit.git"
  },
  "keywords": [
    "actions"
  ],
  "author": "ankri",
  "license": "MIT",
  "dependencies": {
    "@actions/core": "1.2.6",
    "@actions/exec": "1.0.4",
    "@tangro/tangro-github-toolkit": "1.0.3",
    "marked": "1.2.0"
  },
  "devDependencies": {
    "@types/marked": "1.1.0",
    "@types/node": "14.11.5",
    "@zeit/ncc": "0.22.3",
    "ejs": "3.1.5",
    "rimraf": "3.0.2",
    "typescript": "4.0.3"
  }
}
